    MKDIR ~Worldmap~
    COPY ~CtBLiberate/Worldmap/3610areas.tbl~ ~Worldmap/map_mods_areas.tbl~
END

ACTION_IF FILE_EXISTS ~Worldmap/map_mods_links.tbl~ THEN BEGIN
